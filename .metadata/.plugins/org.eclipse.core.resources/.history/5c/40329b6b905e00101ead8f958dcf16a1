	@Test
	void testFibonacci() {
		for(int i = 1; i < 200; i++) {
			System.out.println(MenorCant.fibonacciOptimo(i));
		}
	}
	
	@Test
	void testFibonacciNoOptimo() {
		for(int i = 1; i < 30; i++) {
			System.out.println(MenorCant.fibonacciNoOptimo(i));
		}
	}
