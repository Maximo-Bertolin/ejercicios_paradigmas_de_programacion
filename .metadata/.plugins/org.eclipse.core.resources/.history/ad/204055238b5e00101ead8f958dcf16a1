package programacion_dinamica;

public class menor_cantidad_pasos_a_uno {
	public static int menor_cant(int n) {
		if(n>1) {
			return 1+ min(menor_cant(n-1), menor_cant(n/2), menor_cant(n/3));
		}
		return 0;
	}
	
	public static int min(int a, int b, int c) {
		if(a < b && a < c) {
			return a;
		}else if(b < c) {
			return b;
		}
		return c;
	}
}
